{
  "name": "aleph-server",
  "version": "0.0.1",
  "description": "Collaborative presentation server",
  "main": "index.js",
  "scripts": {
    "test": "npm run lint && npm run testonly",
    "testonly": "mocha --opts support/mocha.opts",
    "testonly:min": "mocha --reporter min --growl --opts support/mocha.opts",
    "test:watch": "echo -ne \"\\033]0;aleph-server test:watch\\007\" && watch-run -i -p 'src/**/*,support/**/*' npm run testonly:min",
    "lint": "eslint src",
    "lint:watch": "echo -ne \"\\033]0;aleph-server lint:watch\\007\" && esw -w 'src/**/*.js'",
    "build:babel": "babel src --ignore __tests__ --out-dir dist",
    "build": "npm run build:babel",
    "start": "echo -ne \"\\033]0;aleph-server watch\\007\" && ./node_modules/.bin/nodemon --watch src src/watch.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/tmpaul06/aleph-server.git"
  },
  "author": "Tharun Mathew Paul",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tmpaul06/aleph-server/issues"
  },
  "homepage": "https://github.com/tmpaul06/aleph-server#readme",
  "dependencies": {
    "async": "1.5.2",
    "bluebird": "3.3.5",
    "body-parser": "1.14.1",
    "compression": "1.6.1",
    "connect-multiparty": "2.0.0",
    "ejs": "2.4.1",
    "express": "4.13.4",
    "express-session": "^1.14.0",
    "express-socket.io-session": "^1.3.1",
    "history": "2.1.1",
    "moment": "^2.13.0",
    "morgan": "1.7.0",
    "newrelic": "1.22.2",
    "node-uuid": "1.4.7",
    "object-assign": "4.1.0",
    "promise": "7.1.1",
    "sanitize-filename": "1.5.3",
    "serve-favicon": "2.3.0",
    "socket.io": "^1.4.8",
    "winston": "2.1.1"
  },
  "devDependencies": {
    "babel-cli": "6.4.0",
    "babel-core": "6.4.0",
    "babel-eslint": "6.0.0-beta.6",
    "babel-plugin-module-alias": "1.4.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-polyfill": "6.3.14",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-stage-0": "6.3.13",
    "babel-register": "6.4.3",
    "chai": "3.5.0",
    "chai-as-promised": "5.3.0",
    "colors": "1.1.2",
    "eslint": "2.4.0",
    "eslint-plugin-babel": "3.1.0",
    "eslint-watch": "2.1.10",
    "minimist": "^1.2.0",
    "mocha": "2.4.5",
    "nodemon": "1.9.1",
    "sinon": "1.17.3",
    "super-request": "1.1.0",
    "tmp": "0.0.28",
    "watch-run": "1.2.4"
  }
}
